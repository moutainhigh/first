			<if test="address != null">
				address = #{address},
			</if>
			<if test="tel != null">
				TEL = #{tel}
			</if>
		</set>
		,LASTUPDATE = now() 
		<where>
			empStatus = 'on' 
			<if test="empId != 0 and empId != '' and empId != null">
				and empid=#{empId}
			</if>
			<if test="empCode != null and empCode!=''">
				and empCode = #{empCode}
