<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />

	<!-- 登录缓存 -->
	<cache 
	name="dpmEhcache" 
	maxEntriesLocalHeap="1000" 
	eternal="false"
	timeToIdleSeconds="1800" 
	timeToLiveSeconds="0" 
	maxEntriesLocalDisk="10000000"
	diskExpiryThreadIntervalSeconds="120" 
	memoryStoreEvictionPolicy="LRU" 
	/>

</ehcache>